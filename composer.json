{
  "name": "rockbuzz/lara-custom-validation",
  "description": "Laravel custom validations",
  "keywords": ["laravel", "validation", "custom validation"],
  "license": "MIT",
  "authors": [
    {
      "name": "TiagoDevWeb",
      "email": "tiago.dweb@gmail.com"
    },
    {
      "name": "Paulo Henrique P. Scheuermann",
      "email": "paetzold44@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rockbuzz\\LaraCustomValidation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "^4.0",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox",
    "cs": "vendor/bin/phpcs"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rockbuzz\\LaraCustomValidation\\ServiceProvider"
      ]
    }
  }
}
